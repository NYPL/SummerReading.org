    {
        linkToRedirect = href;
        var fSaving = SaveNetImaging(80);        
        
        var fResult;
        // If it's a download in IE and if we need to save, then we need to have little bit different code than in FireFox, etc.
        if(fSaving && isDownload && BrowserDetect.browser == "Explorer")
        {
            NotifyServerAJAX();
            thisLink.href = href;
            fResult = true;            
        }
        else        
        {
            PostbackLinkClick();
            fResult = false;
        }

        return fResult;
    }
    
    function ConfirmDeleting()
    {
        return confirm("Are you sure you want delete the image?");
    }
    
    function ConfirmDeletingAll()
    {
        return confirm("Are you sure you want delete all images?");
    }    
    
    function ConfirmStartOver()
    {
        return confirm("Are you sure you want start over? All your changes will be lost."); 
    }
    
    function ResizeTool()
    {
        var availWidth = screen.availWidth;        
        //var availWidth = 1024;
        var tblMain = document.getElementById('tblMain');
        if(availWidth < 1000)
        {
            //alert("700");
            tblMain.style.width = "700px";
        }        
        else if(availWidth < 1100)
        {
            //alert("800");
            tblMain.style.width = "800px";
        }        
        else if(availWidth < 1200)
        {
            tblMain.style.width = "900px";
        }
    }
    
    function HelpHover(fHover)
    {
        document.getElementById("imgHelp").style.display = fHover ? "none" : "inline";
        document.getElementById("imgHelpHover").style.display = fHover ? "inline" : "none";
    }
    </script>            
    
    

