                            "images/blue.png", "images/pink.png", "images/yellow.png",
                            "images/blue.png", "images/pink.png", "images/yellow.png");*/

var arrImageIDs = new Array("stripImg1", "stripImg2", "stripImg3",
                            "stripImg4", "stripImg5", "stripImg6",
                            "stripImg7", "stripImg8", "stripImg9");

function _FindStripImage(aHref)
{
    var img;    
    for(var i = 0; i < aHref.childNodes.length; i++)
    {
        var node = aHref.childNodes[i];
        if(node != null && node.id != null && node.id.indexOf("stripImg") == 0)
        {
            img = node;
            break;
        }
    }    
    return img;
}


function PostbackImageClick()
{   
    if(IsChanged())
    {
        setTimeout(PostbackImageClick, 200);
    }
    else    
    {
        var btnPostback = document.getElementById('ImageStrip1_btnPostback');
        btnPostback.click();
    }
}

function ImageClicked(hlImage)
{
    SaveNetImaging(80);

    var hdSelectedImage = document.getElementById('ImageStrip1_hfSelectedImage');
    var img = _FindStripImage(hlImage);    
    hdSelectedImage.value = img.src;
 
    PostbackImageClick();       
}

var visibleImagesNumber = arrImageIDs.length;
var currImageIndex = 0;
function ImageStripRefresh()
{
    var imgPreload;    
    for(var i = 0; i < visibleImagesNumber; i++)
    {
        var imageTag = document.getElementById(arrImageIDs[i]);
        var imageUrl;
        if(i < arrAllImageUrls.length)
        {            
            imageUrl = arrAllImageUrls[currImageIndex + i];     
            //imgPreload = new Image();                 
            //imgPreload.src = imageUrl;  
        }
        else
        {
            imageUrl = "images/transparent_pixel.gif";            
        }
        imageTag.src = imageUrl;
        
        var tdToHighlight = imageTag.parentNode.parentNode;    
        if(imageUrl.indexOf("conv/" + selectedImageId + "/") != -1)
        {            
            tdToHighlight.style.backgroundColor = selectedColor;        
        }
        else
        {
            // Clear old selection
            tdToHighlight.style.backgroundColor = "";        
        }
    }
    
    // Preload prev & next item
    if(currImageIndex > 0)
    {
        imageUrl = arrAllImageUrls[currImageIndex - 1];     
        imgPreload = new Image();                 
        imgPreload.src = imageUrl;          
    }    
    if(currImageIndex + visibleImagesNumber < arrAllImageUrls.length)
    {
        imageUrl = arrAllImageUrls[currImageIndex + visibleImagesNumber];     
        imgPreload = new Image();                 
        imgPreload.src = imageUrl;              
    }
    
    // Save current selected image
    var hfCurrentImageIndex = document.getElementById('ImageStrip1_hfCurrentImageIndex');
    hfCurrentImageIndex.value = currImageIndex;    
}

function ImageStripScrollBack()
{    
    if(currImageIndex > 0)
    {
        currImageIndex --;        
        ImageStripRefresh();        
    }
}

function ImageStripScrollForward()
{
    if(currImageIndex < arrAllImageUrls.length - visibleImagesNumber)  // Should be replaced with visible images
    {
        currImageIndex ++;        
        ImageStripRefresh();
    }
}

function HighlighImage(aHref, fHighlight)
{       
    //var  imgStrip = aHref.childNodes[0];
    var imgStrip = _FindStripImage(aHref);    
    if(imgStrip.width > 1)
    {        
        var tdToHighlight = aHref.parentNode;            
        if(tdToHighlight.style.backgroundColor != selectedColor && tdToHighlight.style.backgroundColor != HexToDec(selectedColor))
        {
            if(fHighlight)
            {        
                tdToHighlight.style.backgroundColor = "#b2b2b2";
            }
            else    
            {
                tdToHighlight.style.backgroundColor = "";
            }
        }
    }
}


function ResizeStrip()
{
    var availWidth = screen.availWidth;             
    //var availWidth = 1024;
    
    var tdStrip7 = document.getElementById('tdStrip7');
    var tdStrip8 = document.getElementById('tdStrip8');
    var tdStrip9 = document.getElementById('tdStrip9');
    
    visibleImagesNumber = arrImageIDs.length;
    if(availWidth < 1200)
    {
        tdStrip9.style.display = "none";
        visibleImagesNumber--;
    }
    if(availWidth < 1100)
    {
        tdStrip8.style.display = "none";
        visibleImagesNumber--;
    }
    if(availWidth < 1000)
    {
        tdStrip7.style.display = "none";
        visibleImagesNumber--;
    }
    
    var hfVisibleImagesNumber = document.getElementById('ImageStrip1_hfVisibleImagesNumber');
    hfVisibleImagesNumber.value = visibleImagesNumber;
}
