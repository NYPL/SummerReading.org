<script type="text/javascript">
//var selectedColor = "#b8e1b8";//"#6d6d92";
var selectedColor = "#838383";


var selectedImageId = 234445;
var arrAllImageUrls = new Array('t/conv/234445/bd6ba2fb22585eaf443099177a0ff1e6/a2.jpg');
/*"images/blue.png", "images/pink.png", "images/yellow.png",
                            "images/blue.png", "images/pink.png", "images/yellow.png",
